<mvvm:MVVMWindow x:Class="GreaterFileShare.Hosts.WPF.UriAndQRs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UriAndQRs" Height="350" Width="525"
        xmlns:mvvm="clr-namespace:MVVMSidekick.Views;assembly=MVVMSidekick"
        xmlns:vm="clr-namespace:GreaterFileShare.Hosts.WPF.ViewModels" >
    <FrameworkElement.Resources>
        <vm:UriAndQRs_Model x:Key="DesignVM" />
    </FrameworkElement.Resources>
    <Grid x:Name="grid" DataContext="{StaticResource DesignVM}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="12*"/>
            <ColumnDefinition Width="35*"/>
        </Grid.ColumnDefinitions>
        <ListBox ItemsSource="{Binding Hosts}">
            
        </ListBox>
        <StackPanel Grid.Column="1"
                    >

            <Button >Launch</Button>
        </StackPanel>
        

    </Grid>
</mvvm:MVVMWindow>
